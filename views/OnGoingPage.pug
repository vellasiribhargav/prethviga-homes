.project-details-section
  .project-details-container
    .project-features-header
      h1.project-features-header__title Project Features
      p.project-features-header__description
        | Discover the exceptional amenities and features that make Sunset Ridge Residence stand out amenities
        | and features that make Sunset Ridge Residence
    .features-grid
      each features in featureData
        .feature-card
          span.feature-card__icon
            svg(
              width=features.svg.width
              height=features.svg.height
              viewBox=features.svg.viewBox
              fill="none"
              xmlns="http://www.w3.org/2000/svg"
            )
              path(
                d=features.svg.path
                stroke="#C1834E"
                stroke-width="2.66667"
                stroke-linecap="round"
                stroke-linejoin="round"
              )
          span.feature-card__heading #{features.fc_heading}
          span.feature-card__description #{features.fc_description}
  .project-planning-container
    .project-details-header
      h1.project-planning-title Project Features
      p.project-planning-description
        | Discover the exceptional amenities and features that make Sunset Ridge Residence stand out.
    .amenities-list
      each amenity in amenitiesData
        .amenity-card
          span.icon
            svg(
              width = amenity.svg.width
              height = amenity.svg.height
              viewBox = amenity.svg.viewBox
              fill = "none"
              xmlns = "http://www.w3.org/2000/svg"
            )
              path(
                d=amenity.svg.path
                stroke="#C1834E"
                stroke-width="2"
                stroke-linecap="round"
                stroke-linejoin="round"
              )
          span #{amenity.feature}
    .floor-layout
      h1.floor-layout-title Floor Layout
      .floor-image
        img(src='../../assets/images/Rectangle 42.webp' alt='Floor layout image')
  .location-section
